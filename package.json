{
  "name": "petrescue_reactjs",
  "version": "1.0.0",
  "description": "petrescue_reactjs",
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": "12",
    "yarn": "1"
  },
  "author": "Petrescue",
  "license": "MIT",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "yarn analyze:clean",
    "analyze": "node internals/scripts/analyze.js",
    "extract-intl": "node internals/scripts/extract-intl.js",
    "extract-intl-universal": "node -r @babel/register internals/scripts/extract-intl-universal.js",
    "yarncheckversion": "node ./internals/scripts/yarncheckversion.js",
    "preinstall": "yarn yarncheckversion",
    "prebuild": "yarn build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf www",
    "start": "node server",
    "start:tunnel": "cross-env ENABLE_TUNNEL=true yarn start",
    "start:production": "yarn test && yarn build && yarn start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "presetup": "yarn add chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "clean": "shjs ./internals/scripts/clean.js",
    "clean:all": "yarn analyze:clean && yarn test:clean && yarn build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "yarn lint:js && yarn lint:json && yarn lint:md",
    "lint:js": "yarn lint:eslint -- . && yarn lint:stylelint -- \"**/*.{js,jsx}\"",
    "lint:json": "for i in $(find . -name \"*.json\" -a -not -path \"*/certificates/*\" -a -not -path \"*/infrastructure/ansible/files/*\" -a -not -path \"*/infrastructure/lib/*\" -a -not -path \"*/node_modules/*\" -a -not -path \"*/platforms/*\" -a -not -path \"*/plugins/*\" -a -not -path \"*/www/*\") ; do echo $i ; yarn lint:jsonlint $i || exit $? ; done",
    "lint:md": "yarn lint:remark -- .",
    "lint:sh": "yarn lint:shellcheck -- $(find . -name \"*.sh\" -a -not -path \"*/infrastructure/bin/*\" -a -not -path \"*/node_modules/*\" -a -not -path \"*/platforms/*\" | tr \"\n\" \" \")",
    "lint:eslint": "eslint",
    "lint:stylelint": "stylelint",
    "lint:jsonlint": "jsonlint -q",
    "lint:remark": "remark",
    "lint:shellcheck": "shellcheck",
    "lint:staged": "lint-staged",
    "pretest": "yarn test:clean",
    "test:clean": "rimraf coverage",
    "test": "cross-env BABEL_ENV=test jest",
    "test:watch": "cross-env BABEL_ENV=test jest --watch",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn prettier --write",
      "yarn lint:eslint -- --fix",
      "yarn stylelint",
      "git add --force"
    ],
    "*.json": [
      "yarn prettier --write",
      "yarn lint:jsonlint --",
      "git add --force"
    ],
    "*.html": [
      "yarn prettier --write",
      "git add --force"
    ],
    "*.md": [
      "yarn lint:remark --"
    ],
    "*.sh": [
      "yarn lint:shellcheck --"
    ]
  },
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.3",
